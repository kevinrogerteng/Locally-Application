<%= link_to "View All Trips", trips_path %>
<div class= "Trip Information">
	Trip Name: <%= @trip['name'] %> <br>
	Destination: <%= @trip['destination'] %> <br>
	Description: <%= @trip['description'] %> <br>
	<%= link_to "Edit", edit_trip_path(@trip['id']) %>
	<%= link_to "Delete", trip_path(@trip['id']), method: :delete%><br>
</div>
<br>

Available Things to do in <%= @trip['destination'] %><br>

<% @result.each do |business| %>
	<img src=<%= business["image_url"]%> /><br>
	<%= business["name"] %><br>
	<% business["location"]['display_address'].each do |address| %>
		<%= address %> 
	<%end %>
	<br>
	<%= business["display_phone"] %> <br>
	<img src=<%= business["rating_img_url"] %> /> <br>
	<%= link_to "More Info", business["url"], :target => '_blank' %><br>
<%end %>

	


	
