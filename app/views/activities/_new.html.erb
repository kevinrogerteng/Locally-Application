	<%= form_for :activity, method: :post, url: activities_path do |f| %>
		<%= label_tag(:name, "Name") %> <%= f.text_field :name%><br>
		<%= label_tag(:address, "Address") %> <%= f.text_field :address %><br>
		<%= label_tag(:biz_url, "Business URL") %> <%= f.text_field :biz_url %><br>
		<%= label_tag(:phone, "Phone") %> <%= f.text_field :phone %><br>
		<% f.hidden_field :tripid, value: @trip.to_i %>
		<% binding.pry %>
		<%= f.submit "Create Activity"%>
	<%end%>
